{
  "name": "@meform/db",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "generate": "npx dotenv-cli -e ../../.env -- prisma generate",
    "migrate": "npx dotenv-cli -e ../../.env -- prisma migrate dev",
    "migrate:deploy": "npx dotenv-cli -e ../../.env -- prisma migrate deploy",
    "seed": "npx dotenv-cli -e ../../.env -- tsx src/seed.ts",
    "studio": "npx dotenv-cli -e ../../.env -- prisma studio",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "dotenv-cli": "^11.0.0",
    "prisma": "^5.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
